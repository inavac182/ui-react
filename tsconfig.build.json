{
  "compilerOptions": {
    "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "importHelpers": true,
    "strictNullChecks": true,
    "noUnusedLocals": false,
    "declaration": true,
    "declarationMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "examples",
    "src/gatsby-theme-docz"
  ],
  "paths": {
    "@uireact/*": [
      "packages/**/dist"
    ],
  }
}