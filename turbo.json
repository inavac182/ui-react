{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build:docs": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "build": {
      "cache": true
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "docs:dev": {
      "cache": true
    },
    "docs:clean": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}