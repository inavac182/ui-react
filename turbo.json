{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build:docs": {
      "dependsOn": [
        "^build:publish"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "build:publish": {
      "dependsOn": ["^build:publish"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "build": {
      "cache": true
    },
    "test": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev:storybook": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "dev:docs": {
      "cache": true
    },
    "docs:clean": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "watch": {
      "outputs": ["dist/**"],
      "persistent": true,
      "cache": true
    }
  }
}