{
  "name": "uireact",
  "repository": "https://github.com/inavac182/uireact/",
  "license": "SEE LICENSE",
  "scripts": {
    "bootstrap": "npx lerna bootstrap --hoist",
    "bootstrap:quick": "npm run bootstrap -- --scope=@uireact/*",
    "build": "npx lerna run build",
    "clean": "rm -rf .docz && npm run clean:packages",
    "clean:packages": "npx lerna run clean -- --yes",
    "dev": "npm run clean && npm run tsc && docz dev",
    "dev:quick": "docz dev",
    "docz:build": "npm run clean && npm run tsc && docz build",
    "docz:serve": "docz build && docz serve",
    "lint": "npx lerna run lint --stream",
    "preinstall": "npm run clean",
    "typescheck": "lerna run tsc",
    "tsc": "npx lerna run tsc",
    "test": "npm run tsc && npx lerna run test",
    "test:jest": "npx lerna run test",
    "test:tsc": "npx lerna run tsc --stream",
    "test:coverage": "npx lerna run test:coverage",
    "prepare": "husky install"
  },
  "devDependencies": {
