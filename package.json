{
  "name": "uireact",
  "repository": "https://github.com/inavac182/ui-react/",
  "license": "SEE LICENSE",
  "scripts": {
    "bootstrap": "npx lerna bootstrap --hoist",
    "bootstrap:quick": "npm run bootstrap -- --scope=@ui-react/*",
    "build": "npx lerna run build --concurrency=1 --stream",
    "build:quick": "npm run build -- --scope=@ui-react/*",
    "clean": "npm run clean:artifacts && npm run clean:packages",
    "clean:artifacts": "npx lerna run clean --parallel",
    "clean:packages": "npx lerna clean --yes",
    "dev": "npm run clean && npm run tsc && docz dev",
    "docz:build": "npm run clean && npm run tsc && docz build",
    "docz:serve": "docz build && docz serve",
    "watch": "npx lerna run tsc --stream && lerna run --parallel watch",
    "lint": "npx lerna run lint --stream",
    "preinstall": "npm run clean",
    "typescheck": "lerna run tsc",
    "tsc": "npx lerna run tsc --concurrency=1 --stream",
    "test": "npm run tsc && npx lerna run test --concurrency=1 --stream",
    "test:tsc": "npx lerna run tsc --stream",
    "test:quick": "npx lerna run test:quick --concurrency=1 --stream",
    "test:coverage": "npx lerna run test:coverage --parallel",
    "test:watch": "npx lerna run test:watch --parallel",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.0.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.1",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@uireact/foundation": "file:packages/foundation",
    "@uireact/card": "file:packages/card",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "babel-plugin-styled-components": "^1.12.0",
    "commitlint": "^12.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-config-xo": "^0.29.1",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-config-xo-space": "^0.24.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gatsby-plugin-styled-components": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "lerna": "^4.0.0",
    "lint": "^0.7.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "styled-components": "^5.2.3",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.5.4",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.2.3"
  },
  "author": "http://github.com/inavac182",
  "dependencies": {
    "docz": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3"
  }
}
