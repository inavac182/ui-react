{
  "name": "uireact",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:packages": "turbo run build:publish",
    "build:docs": "node ./scripts/compile-packages-metadata.js && node ./scripts/build-docs.js && turbo run build:docs",
    "build:publish": "turbo run build:publish",
    "clean": "turbo run clean && rm -rf ./node_modules/",
    "dev:docs": "nodemon --ignore apps -e mdx --exec \"turbo run watch --concurrency 29\"",
    "dev:storybook": "turbo run dev:storybook",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "publish:packages": "lerna publish",
    "publish:versions": "lerna version",
    "test": "turbo run build:publish && turbo run test",
    "test:packages": "turbo run tsc --filter=packages/* && turbo run test --continue"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@uireact/badge": "workspace:*",
    "@uireact/button": "workspace:*",
    "@uireact/card": "workspace:*",
    "@uireact/charts": "workspace:*",
    "@uireact/confirm-dialog": "workspace:*",
    "@uireact/create-next-app": "workspace:*",
    "@uireact/create-remix-app": "workspace:*",
    "@uireact/datepicker": "workspace:*",
    "@uireact/dialog": "workspace:*",
    "@uireact/docs-tools": "workspace:*",
    "@uireact/expando": "workspace:*",
    "@uireact/flex": "workspace:*",
    "@uireact/form": "workspace:*",
    "@uireact/foundation": "workspace:*",
    "@uireact/framer-animations": "workspace:*",
    "@uireact/grid": "workspace:*",
    "@uireact/header": "workspace:*",
    "@uireact/icons": "workspace:*",
    "@uireact/list": "workspace:*",
    "@uireact/menu": "workspace:*",
    "@uireact/navbar": "workspace:*",
    "@uireact/notifications": "workspace:*",
    "@uireact/progress-indicator": "workspace:*",
    "@uireact/separator": "workspace:*",
    "@uireact/table": "workspace:*",
    "@uireact/tabs": "workspace:*",
    "@uireact/text": "workspace:*",
    "@uireact/validator": "workspace:*",
    "@uireact/view": "workspace:*",
    "add": "^2.0.6",
    "eslint": "^8.28.0",
    "eslint-config-turbo": "latest",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "next": "14.1.3",
    "pnpm": "^8.15.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "turbo": "^1.8.3"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.1",
    "framer-motion": "^11.0.18",
    "lerna": "^8.1.2",
    "nodemon": "^3.1.0",
    "react": "^18",
    "react-dom": "^18",
    "styled-components": "6.0.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "tsc-watch": "^6.2.0"
  }
}