{
  "name": "@uireact/icons",
  "description": "Icons package from @uireact library",
  "repository": "https://github.com/inavac182/ui-react",
  "license": "MIT",
  "version": "0.2.6",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run tsc",
    "build:svgs": "npx @svgr/cli --typescript --icon --out-dir src/public/svgs/ -- svgs/",
    "clean": " rm -rf dist node_modules coverage src/public/svgs/",
    "eslint": "eslint ./src/**/*.{ts,tsx} ./__tests__/**/*.tsx --max-warnings=0",
    "lint": "npm run tsc && npm run eslint && npm run lint:styles",
    "lint:styles": "stylelint ./src/**/*.tsx",
    "test": "jest",
    "tsc": "npm run build:svgs && tsc",
    "tsc:watch": "tsc --watch",
    "watch": "npm run tsc:watch"
  },
  "devDependencies": {
    "@uireact/foundation": "^0.14.0"
  },
  "peerDependencies": {
    "@types/styled-components": "^5.1.0",
    "@uireact/foundation": "^0.14.0",
    "react": ">= 17 < 19",
    "react-dom": ">= 17 < 19",
    "styled-components": "^5.1.1",
    "tslib": "^2.5.0"
  }
}
