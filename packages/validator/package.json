{
  "name": "@uireact/validator",
  "description": "UiValidator package from @uireact library",
  "repository": "https://github.com/inavac182/ui-react",
  "license": "GNU",
  "version": "1.1.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:publish": "tsup src/index.ts --format cjs,esm --dts",
    "clean": " rm -rf dist node_modules coverage",
    "dev": "tsc",
    "eslint": "eslint ./src/**/*.ts ./__tests__/**/*.ts --max-warnings=0",
    "lint": "npm run tsc && npm run eslint",
    "test": "jest",
    "watch": "tsc --watch"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18",
    "react-dom": "^18",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "tslib": "^2.5.0"
  }
}