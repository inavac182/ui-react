{
  "name": "@uireact/badge",
  "description": "Badge component from @uireact library",
  "repository": "https://github.com/inavac182/uireact",
  "license": "GNU",
  "version": "1.4.4",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "files": [
    "src"
  ],
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": " rm -rf dist node_modules coverage",
    "eslint": "eslint ./src/**/*.{ts,tsx} ./__tests__/**/*.tsx --max-warnings=0",
    "lint": "npm run tsc && npm run eslint && npm run lint:styles",
    "lint:styles": "stylelint ./src/**/*.tsx",
    "test": "jest",
    "tsc": "tsc",
    "watch": "tsc --watch"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@uireact/foundation": "workspace:*",
    "react": "^18",
    "react-dom": "^18",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "@uireact/foundation": "^2.0.1",
    "react": ">= 17 < 19",
    "react-dom": ">= 17 < 19",
    "styled-components": ">= 4 <= 6",
    "tslib": "^2.5.0"
  }
}