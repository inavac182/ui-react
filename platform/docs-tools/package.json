{
  "name": "@uireact/docs-tools",
  "description": "@uireact/docs-tools package for @uireact docs",
  "repository": "https://github.com/inavac182/uireact",
  "license": "GNU",
  "version": "0.0.1",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src"
  ],
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": " rm -rf dist node_modules coverage",
    "dev": "tsc"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@uireact/badge": "workspace:*",
    "@uireact/icons": "workspace:*",
    "@uireact/flex": "workspace:*",
    "@uireact/text": "workspace:*",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "react": "^18",
    "react-dom": "^18",
    "styled-components": "^6.1.8",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  }
}